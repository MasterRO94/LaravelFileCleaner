{
    "name": "masterro/laravel-file-cleaner",
    "description": "Laravel console command for deleting temp files and associated model instances.",
    "license": "MIT",
    "keywords": ["laravel", "files", "delete", "clean"],
    "authors": [
        {
            "name": "Ihoshyn Roman",
            "email": "igoshin18@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.1,<5.6",
        "illuminate/filesystem": "^5.1,<5.6",
        "illuminate/console": "^5.1,<5.6",
        "nesbot/carbon": "^1.21"
    },
	"autoload": {
		"psr-4": {
			"MasterRO\\LaravelFileCleaner\\": "src/"
		}
	},
    "extra": {
      "laravel": {
        "providers": [
          "MasterRO\\LaravelFileCleaner\\FileCleanerServiceProvider"
        ]
      }
    }
}
