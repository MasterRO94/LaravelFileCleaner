{
	"name": "masterro/laravel-file-cleaner",
	"description": "Laravel console command for deleting temp files and associated model instances.",
	"license": "MIT",
	"keywords": [
		"laravel",
		"files",
		"delete",
		"clean"
	],
	"authors": [
		{
			"name": "Ihoshyn Roman",
			"email": "igoshin18@gmail.com"
		}
	],
	"require": {
		"php": ">=7.2.5",
		"illuminate/support": "^6.0|^7.0|^8.0",
		"illuminate/filesystem": "^6.0|^7.0|^8.0",
		"illuminate/console": "^6.0|^7.0|^8.0",
		"illuminate/database": "^6.0|^7.0|^8.0",
		"nesbot/carbon": "^1.21|^2.1"
	},
	"require-dev": {
		"orchestra/testbench": "^v5.0|^v6.0",
		"mockery/mockery": "^1.3.1|^1.4.2"
	},
	"suggest": {
		"masterro/laravel-fresh-start": "v1.0.0",
		"masterro/laravel-flashes": "v0.1.0"
	},
	"autoload": {
		"psr-4": {
			"MasterRO\\LaravelFileCleaner\\": "src/",
			"MasterRO\\LaravelFileCleaner\\Tests\\": "tests/"
		},
		"classmap": [
			"tests/Database/migrations"
		]
	},
	"extra": {
		"laravel": {
			"providers": [
				"MasterRO\\LaravelFileCleaner\\FileCleanerServiceProvider"
			]
		}
	}
}
